{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/games",
      "destination": "/games.html"
    },
    {
      "source": "/windows",
      "destination": "/windows.html"
    },
    {
      "source": "/reinstall-windows",
      "destination": "/reinstall-windows.html"
    },
    {
      "source": "/404",
      "destination": "/404.html",
      "statusCode": 404
    }
  ]
}
